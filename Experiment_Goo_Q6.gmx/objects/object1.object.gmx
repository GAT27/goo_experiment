hspeed=0; vspeed=0;

movex=6+(2*joystick_xpos(1));
movey=6+(5*joystick_ypos(1));
movexy=floor(movex+movey);
countd=0;

if !playb switch(movexy)
{
    case 14: countd=1; break;
    case 9: countd=2; break;
    case 7: countd=3; break;
    case 5: countd=4; break;
    case 10: countd=5; break;
    case 15: countd=6; break;
    case 17: countd=7; break;
    case 19: countd=8; break;
    default: countd=0;
}

if joystick_check_button(1, 4)
{
    playb=1;
    file_text_close(file);
    file = file_text_open_read(working_directory + "record.txt");
}

if joystick_check_button(1, 3) and !playb
{
    //file = file_text_open_append(working_directory + "record.txt");
    file_text_write_real(file, countd);
    file_text_writeln(file);
    //file_text_close(file);
}

if playb
{
    countd = file_text_read_real(file);
    file_text_readln(file);
    if file_text_eof(file)
    {
        playb=0;
        file_text_close(file);
        file = file_text_open_append(working_directory + "record.txt");
    }
}

switch(countd)
{
    case 1: hspeed=2; break;
    case 2: hspeed=2; vspeed=-2; break;
    case 3: vspeed=-2; break;
    case 4: hspeed=-2; vspeed=-2; break;
    case 5: hspeed=-2; break;
    case 6: hspeed=-2; vspeed=2; break;
    case 7: vspeed=2; break;
    case 8: hspeed=2; vspeed=2; break;
    default: hspeed=0; vspeed=0;
}
